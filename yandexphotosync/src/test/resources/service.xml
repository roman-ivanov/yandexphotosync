 <app:service xmlns:app="http://www.w3.org/2007/app" xmlns:atom="http://www.w3.org/2005/Atom">
  <app:workspace>
    <atom:title>test-sync на Яндекс.Фотках</atom:title>
    <app:collection href="http://api-fotki.yandex.ru/api/users/test-sync/albums/" id="album-list">
      <atom:title>Все альбомы пользователя test-sync</atom:title>
      <app:accept>application/atom+xml; type=entry, application/json; type=entry</app:accept>
    </app:collection>
    <app:collection href="http://api-fotki.yandex.ru/api/users/test-sync/photos/" id="photo-list">
      <atom:title>Все фотографии пользователя test-sync</atom:title>
      <app:accept>image/*</app:accept>
      <app:categories scheme="http://api-fotki.yandex.ru/api/users/test-sync/tags/" />
    </app:collection>
    <app:collection href="http://api-fotki.yandex.ru/api/users/test-sync/tags/" id="tag-list">
      <atom:title>Все теги пользователя test-sync</atom:title>
      <app:accept />
    </app:collection>
  </app:workspace>
</app:service>